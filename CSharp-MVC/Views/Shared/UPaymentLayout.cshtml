                                        </span>
                                    </a>
                                </div>
                                <!-- Account End -->
                                <div class="block-wishlist action">
                                    <!-- Sản phẩm yêu thích Start -->
                                    <a class="icon-link icon-link-2" href="wishlist.html">
                                        <i class="flaticon-heart"></i>
                                        <span class="count count-2">0</span>
                                        <span class="text">
                                            <span class="sub">Favorite</span>
                                            My Wishlist
                                        </span>
                                    </a>
                                    <!-- Sản phẩm yêu thích End -->
                                </div>
                                <div class="block-cart action">
                                    @if(session != null)
                                    {
                                        <!-- Giỏ hàng Start -->
                                        float subtotal = 0;
                                        int count = 0;
                                        @foreach(var c in Model.Cart)
                                        {
                                            if(c.UserID == customer.UserID)
                                            {
                                                count += 1;
                                                foreach(var p in Model.Products)
                                                {
                                                    if(p.ProductID == c.ProductID)
                                                    {
                                                        subtotal += p.Price;
                                                    }
                                                }
                                            }
                                            
                                        }
                                        <a class="icon-link icon-link-2" asp-area="" asp-controller="UCart" asp-action="Index">
                                            <i class="flaticon-shopping-bag"></i>
                                            <span class="count count-2">@count</span>
                                            <span class="text">
                                                <span class="sub">Your Cart:</span>
                                                $@subtotal
                                            </span>
                                        </a>
                                        <div class="cart">
                                            <div class="cart__mini">
                                                <ul>
                                                    <li>
                                                        <div class="cart__title">
                                                            <h4>Your Cart</h4>
                                                            <span>(@count Item in Cart)</span>
                                                        </div>
                                                    </li>
                                                
                                                    @foreach(var c in Model.Cart)
                                                    {
                                                        if(c.UserID == customer.UserID)
                                                        {
                                                            <li>
                                                                <div class="cart__item d-flex justify-content-between align-items-center">
                                                                    @foreach(var p in Model.Products)
                                                                    {
                                                                        if(p.ProductID == c.ProductID)
                                                                        {
                                                                            <div class="cart__inner d-flex">
                                                                                <div class="cart__thumb">
                                                                                    <a asp-action="Index" asp-controller="UProductDetail" asp-route-id="@p.ProductID">
                                                                                        <img src="~/Image/home/products/@p.Picture" alt="...">
                                                                                    </a>
                                                                                </div>
                                                                                <div class="cart__details">
                                                                                    <h6><a asp-action="Index" asp-controller="UProductDetail" asp-route-id="@p.ProductID">@p.ProductName</a></h6>
                                                                                    <div class="cart__price">
                                                                                        <span>@p.Price</span>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        }
                                                                    }
                                                                    <div class="cart__del">
                                                                        <a href="@Url.Action("Delete", "UHome", new { cartId = @c.CartID })"><i class="fal fa-times"></i></a>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                        }
                                                        
                                                    }
                                                    <li>
                                                        <div class="cart__sub d-flex justify-content-between align-items-center">
                                                            <h6>Subtotal</h6>
                                                            <span class="cart__sub-total">$@subtotal</span>
                                                        </div>
                                                    </li>
                                                    <li>
                                                        <a asp-controller="UCart" asp-action="Index" class="wc-cart mb-10">View cart</a>
                                                        <a asp-controller="UPayment" asp-action="Index" class="wc-checkout">Checkout</a>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                        <!-- Giỏ hàng End -->
                                    }
                                    else
                                    {
                                        <!-- Giỏ hàng Start -->
                                        <a class="icon-link icon-link-2" asp-controller="USignin" asp-action="Index">
                                            <i class="flaticon-shopping-bag"></i>
                                            <span class="count count-2">0</span>
                                            <span class="text">
                                                <span class="sub">Your Cart:</span>
                                            </span>
                                        </a>
                                        <!-- Giỏ hàng End -->
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
